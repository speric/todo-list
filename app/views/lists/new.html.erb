<h2>Create New List</h2>

<%= form_for @list do |form| %>
<fieldset>
  <%= render "shared/form_errors", :object => @list %>

	<div class="control-group">
	  <%= form.label :name, :class => 'control-label' %>
	  <div class="controls">
	    <%= form.text_field :name, :class => 'text_field' %>
	  </div>
	</div>

  <%= form.submit "Create List", :class => 'btn btn-primary' %>
</fieldset>
<% end %>